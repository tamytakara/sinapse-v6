<p id="notice"><%= notice %></p>

<h1>Votos</h1>

<table>
  <thead>
    <tr>
      <th>Positivo</th>
      <th>Resolução</th>
      <th>Usuário</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @votos.each do |voto| %>
      <tr>
        <td><%= voto.positivo %></td>
        <td><%= voto.resolution.desc %></td>
        <td><%= voto.user.username %></td>
        <td><%= link_to 'Show', voto %></td>
        <td><%= link_to 'Edit', edit_voto_path(voto) %></td>
        <td><%= link_to 'Destroy', voto, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Voto', new_voto_path %>
