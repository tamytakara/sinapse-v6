<p id="notice"><%= notice %></p>

<h1>Vestibulares</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Descrição</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admission_exams.each do |admission_exam| %>
      <tr>
        <td><%= admission_exam.name %></td>
        <td><%= admission_exam.desc %></td>
        <td><%= link_to 'Show', admission_exam %></td>
        <td><%= link_to 'Editar', edit_admission_exam_path(admission_exam) %></td>
        <td><%= link_to 'Excluir', admission_exam, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Cadastrar Vestibular', new_admission_exam_path %>
